<h1><%= @place.name %></h1>

   <% for post in @place.posts %>
    <h3>
      <%= post.title %>
    </h3>
    <p>
      <%= post.description %>
    </p>
    <p>
      <%= post.posted_on %>
    </p>

   <% end %>

<p>
  <a href="/posts/new?place_id=<%= @place.id %>">Input New Post</a>
</p>

<p>
  <a href="/places">Go Back to Places</a>
</p>