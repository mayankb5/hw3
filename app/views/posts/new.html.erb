<h1>Input New Post</h1>


<%= form_with model: @post do |form| %>
  <p>
    <%= form.label "Post Title" %>
    <%= form.text_field "title" %>
  </p>
  
  <p>
    <%= form.label "Post Description" %>
    <%= form.text_field "description" %>
  </p>

  <p>
    <%= form.label "Posted On" %>
    <%= form.text_field "posted_on" %>
  </p>

  <p>
    <%= form.submit %>
  <p>

<% end %>